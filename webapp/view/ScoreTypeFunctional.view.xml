<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="com.sap.build.standard.endUserApp.controller.ScoreTypeFunctional" xmlns="sap.m">
    <Page showHeader="true" title="" showFooter="true" showNavButton="true" navButtonPress="_onPageNavButtonPress">
        <content>
          <List id="sapdomein" items="{path: '/ExTypeFunctionalSet' }" selectionChange="onItemSelect" growing="true" growingScrollToLoad="true" >
				      	<items>
			<StandardListItem id="sapdomeinid" title="{Naam}" description="" info="" infoState="Success" type="Navigation" selected="true" press="_onStandardListItemPress"/>
            </items>
            </List>
        </content>
         <ProgressIndicator width="100%" percentValue="75" showValue="true" displayValue="Progress" state="Success" enabled="true" visible="true"/>
        <footer>
            <Bar design="Auto">
                <contentLeft/>
                <contentMiddle>
                    <Button text="Save" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"/>
                </contentMiddle>
                <contentRight/>
            </Bar>
        </footer>
        <headerContent>
            <Button text="Add projects" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="_onButtonPress"/>
        </headerContent>
        <subHeader/>
        <customHeader/>
    </Page>
</mvc:View>
