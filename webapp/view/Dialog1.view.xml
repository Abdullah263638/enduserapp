<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="com.sap.build.standard.endUserApp.controller.Dialog1" xmlns="sap.m">
    <Dialog contentHeight="150px" horizontalScrolling="true" showHeader="true" state="None" stretch="false" title="News" type="Standard" verticalScrolling="true">
        <content>
            <Text text="U post wordt doorgestuurd naar de NewsLetter." width="auto" maxLines="2" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
        </content>
        <beginButton/>
        <buttons>
            <Button text="OK" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="_onButtonPress"/>
            <Button text="Cancel" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="_onButtonPress1"/>
        </buttons>
        <customHeader/>
        <endButton/>
        <subHeader/>
    </Dialog>
</mvc:View>
